/*Modelo entidad relacion */
nombre() PK=() FK={}

Producto(codigo, nombre, costo)  PK=(codigo)
MateriaPrima(codigo, descripcion, costoBase)  PK=(codigo)
Formula(CodigoProductoFinal, CodigoProductoBase, CodigoMateriaPrima) PK=(CodigoProductoFinal, CodigoProductoBase, CodigoMateriaPrima) FK={CodigoProductoFinal=>Producto.codigo, CodigoProductoBase=>Producto.codigo, CodigoMateriaPrima=>MateriaPrima.codigo}

IVA(ID,descripcion,valor) PK=(ID)
Cliente(CUIT, RazonSocial, domicilio, telefono, ID_IVA) PK=(CUIT) FK={ID_IVA=>IVA.ID}
Pedido(NumeroPedido, descripcion, fecha) PK=(NumeroPedido)
Item(IDItem, cantidad, PrecioUnitario, CodigoProducto) PK=(IDItem) FK={CodigoProducto=>Producto.codigo}
Solicita(CUITCliente, NumeroPedido, IDItem)  PK=(CUITCliente, NumeroPedido, IDItem) FK={CUITCliente=>Cliente.CUIT, NumeroPedido=>Pedido.NumeroPedido,IDItem=>Item.IDItem}

OrdenFabricacion(NumeroPartida, fechaOrden, fechaVencimiento) PK=(NumeroPartida)
Fabricacion(NumeroPedido, IDItem, NumeroPartida) PK=(NumeroPedido, IDItem) FK={NumeroPedido=>Pedido.NumeroPedido, IDItem=>Item.IDItem, NumeroPartida=>OrdenFabricacion.NumeroPartida}

/* Punto extra 
Proveedor() PK=() FK={}
*/


Proveedor(CUIT, razonSocial, domicilio, mail) PK=(CUIT) 
Lista(CodMP, precio, fecha) PK=(CodMP) FK={}
Relacion(CUIT, Lista, MateriaPrima) PK=(CUIT, Lista) FK={CUIT=>Proveedor.CUIT, Lista=>ListaCodMP, MateriaPrima=>MateriaPrima.codigo}

